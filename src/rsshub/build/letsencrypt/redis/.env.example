# === BASE CONFIGURATION ===
# Project name for Docker Compose
COMPOSE_PROJECT_NAME=rsshub

NODE_ENV=production
CACHE_TYPE=memory
# === LETSENCRYPT CONFIGURATION ===
# Nginx proxy configuration
VIRTUAL_PORT=1200
VIRTUAL_HOST=rsshub.example.com
VIRTUAL_PROTO=https

# Let's Encrypt SSL configuration
LETSENCRYPT_HOST=rsshub.example.com
LETSENCRYPT_EMAIL=john.smith@mail.com
#Redis
CACHE_TYPE=redis
REDIS_URL=redis://rsshub-redis:6379/